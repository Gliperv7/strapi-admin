{
  "kind": "collectionType",
  "collectionName": "dog_categories",
  "info": {
    "singularName": "dog-category",
    "pluralName": "dog-categories",
    "displayName": "dog-category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "weight": {
      "type": "string"
    },
    "dog_breeds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::dog-breed.dog-breed",
      "mappedBy": "dog_categories"
    }
  }
}
